<% layout("layout") -%>
<h1><%= title %></h1>
<ul>
  <% for(author in author_list){ %>
  <li>
    <a href="<%= author_list[author].getAuthorUrl %>">
      <%= author_list[author].getFullName %>
    </a>
    (<%= author_list[author].date_of_birth_formatted %> -
    <%= author_list[author].date_of_death_formatted %>)
  </li>
  <% } %>
</ul>
